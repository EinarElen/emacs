alias mkcd eshell/mkdir -p $* ; cd $1
alias md eshell/mkdir -p $*
alias rmv rm -v $*
alias mvv mv -iv $*
alias cpv cp -iv $*
alias d dired $1
alias FF sudo-find-file $1
alias ff find-file $1
alias dnf sudo dnf $*
alias clean-emacs rm -r #*# *~
alias clean-latex rm -r *.aux *.bbl *.blg *.pyg *.out *.log *.toc *.lof *.lot *.fdb_latexmk *.fls _minted*
alias ll ls -lh $*
alias la ls -lAh $*
alias l ls -1 $*
